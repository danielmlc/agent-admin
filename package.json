  "scripts": {
    "build": "webpages-packer build && webpages-server build",
    "start": "node ./dist/controllers/main.js",
    "start:dev": "webpages-server dev -w",
    "dev": "webpages-packer --env dev --watch true --include server,login",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "clean": "rimraf dist && rimraf logs && rimraf node_modules"
  },
  "dependencies": {
    "@alicloud/dysmsapi20170525": "^4.2.0",
    "@alicloud/openapi-client": "^0.4.15",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/typeorm": "^10.0.2",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.4.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-wechat": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "svg-captcha": "^1.4.0",
    "tencentcloud-sdk-nodejs": "^4.1.149",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20.3.1",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@cs/js-eslint-config-library": "^0.2.2",
    "@cs/webpages-packer": "^4.0.2",
    "rimraf": "^6.1.2",
    "eslint": "^9.39.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.3",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "element-plus": "2.8.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@app/sms(|/.*)$": "<rootDir>/libs/sms/src/$1",
      "^@app/captcha(|/.*)$": "<rootDir>/libs/captcha/src/$1"
    }
  }
}